{:site-title                   "Clojure Guides"
 :author                       "Multiple Authors"
 :description                  "Clojure Documentation"
 :site-url                     "https://clojure-doc.org/"
 :post-root                    "posts"
 :page-root                    "articles"
 :post-root-uri                "posts"
 :page-root-uri                "articles"
 :tag-root-uri                 "tags"
 :author-root-uri              "authors"
 :public-dest                  "public"
 :blog-prefix                  ""
 :rss-name                     "feed.xml"
 :rss-filters                  ["cryogen"]
 :recent-posts                 0
 :post-date-format             "yyyy-MM-dd"
 :archive-group-format         "yyyy MMMM"
 :sass-src                     []
 :sass-path                    "sass"
 :theme                        "blue"
 :resources                    ["img" "assets"]
 :keep-files                   [".git" ".github" "README.md" "Klipse.md" "CNAME" "CONTRIBUTING.md" "favicon.ico"]
 :disqus?                      false
 :disqus-shortname             ""
 :ignored-files                [#"\.#.*" #".*\.swp$"]
 :previews?                    false
 :posts-per-page               5
 :blocks-per-preview           2
 :clean-urls                   :trailing-slash
 :collapse-subdirs?            false
 :hide-future-posts?           true
 :klipse                       {:settings {:selector ".klipse-clojure" }}
 :description-include-elements #{:p :h1 :h2 :h3 :h4 :h5 :h6}
 :debug?                       false}
